{
    "name": "2FA Autofill",
    "version": "0.0.0",
    "description": "",
    "manifest_version": 2,
    "permissions": [
        "cookies",
        "webRequest",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["index.js"]
    }],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}